{
  "manifest_version": 2,
  "name": "エンジニアのためのノイズ除去",
  "description": "Google検索で特定サイトを非表示にします",
  "version": "1.0",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content_scripts.js"],
      "css": ["content_css.css"]
    }
  ],
  "permissions": [
    "https://www.google.com/search*"
  ]
}